{
  "name": "@demo/api",
  "version": "0.1.0",
  "private": true,
  "description": "Demo API Library",
  "license": "MIT",
  "scripts": {
    "format": "prettier 'lib/**/*.ts' --write",
    "generate": "yarn generate-api && yarn lint && yarn format",
    "generate-api": "cd ../../tools/api-client-generator && yarn generate",
    "lint": "eslint . --ext .jsx,.js,.tsx,.ts --cache --fix",
    "test": "jest src --updateSnapshot --passWithNoTests",
    "test-ci": "jest src --ci --passWithNoTests",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@tanstack/react-query": "^4.29.3",
    "@types/promise-retry": "^1.1.3",
    "http-status": "^1.6.2",
    "msw": "^1.2.1",
    "promise-retry": "^2.0.1",
    "ts-custom-error": "^3.3.1",
    "typescript": "*"
  },
  "engines": {
    "node": "^20",
    "yarn": ">=1"
  },
  "volta": {
    "node": "20.0.0",
    "yarn": "1.22.19"
  }
}
